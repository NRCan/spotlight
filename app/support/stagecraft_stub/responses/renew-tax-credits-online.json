  "strapline": "Dashboard",
  "title": "Renew tax credits online",
  "description": "Tax credits are payments from the government to people who are either responsible for at least 1 child or on a low income. This dashboard shows statistics about the online tax credit renewals service.",
  "department": {
    "title": "HM Revenue and Customs",
    "abbr": "HMRC"
  },
  "relatedPages": {
    "transaction": {
      "title": "Renew tax credits online",
      "url" : "https://www-qa.tax.service.gov.uk/tax-credits/renewal"
    },
    "improve-dashboard-message": true
  },
  "customer-type": "Individuals",
  "business-model": "Taxpayers",
