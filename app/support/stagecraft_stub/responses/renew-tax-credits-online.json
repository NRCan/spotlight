        "y": [
          {
            "label": "Single claimant",
            "categoryId": "/tax-credits/renewal/declaration",
            "format": "integer"
          },
          {
            "label": "Joint claimant",
            "categoryId": "/tax-credits/renewal/dual/declaration",
            "format": "integer"
          }
        ]
      }
    },
    {
      "slug": "users-at-each-step-joint",
