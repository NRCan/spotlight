    {
      "slug": "application-volumetrics",
      "module-type": "grouped_timeseries",
      "title": "Applications processed per week",
      "description": "<span class=\"group0\">Paper (CIS)</span> and <span class=\"group1\">digital (CCMS)</span> applications over time",
      "category": "channel",
      "value-attribute": "count:sum",
      "show-line-labels": true,
      "use_stack": true,
      "info": [
        "Data source: Ministry of Justice",
        "Shows the number of applications processed, broken down by channel",
        "Digital: applications made online (CCMS)",
        "Paper form: applications made using a paper form (CIS)"
      ],
      "axes": {
        "x": {
          "label": "Date",
          "key": [
            "_start_at",
            "_end_at"
          ],
          "format": "date"
        },
        "y": [
          {
            "label": "Paper (CIS)",
            "groupId": "paper",
            "format": "integer"
          },
          {
            "label": "Digital (CCMS)",
            "groupId": "digital",
            "format": "integer"
          }
        ]
      },
      "data-source": {
        "data-group": "legal-aid",
        "data-type": "transactions-by-channel",
        "query-params": {
          "period": "week",
          "filter_by": [
            "service:legal-aid-civil-claims",
            "transaction:applications",
            "transaction_state:processed"
          ],
          "start_at": "2014-03-31T00:00:00Z",
          "group_by": "channel",
          "collect": [
            "count:sum"
          ]
        }
      }
    },
    {
      "slug": "rejected-volumetrics",
      "module-type": "grouped_timeseries",
      "title": "Applications rejected per week",
      "description": "<span class=\"group0\">Paper (CIS)</span> and <span class=\"group1\">digital (CCMS)</span> applications rejected due to incomplete or incorrect information over time",
      "category": "channel",
      "value-attribute": "count:sum",
      "show-line-labels": true,
      "use_stack": true,
      "info": [
        "Data source: Ministry of Justice",
        "Shows the number of applications rejected, broken down by channel",
        "Digital: applications made online (CCMS)",
        "Paper form: applications made using a paper form (CIS)"
      ],
      "axes": {
        "x": {
          "label": "Date",
          "key": [
            "_start_at",
            "_end_at"
          ],
          "format": "date"
        },
        "y": [
          {
            "label": "Paper (CIS)",
            "groupId": "paper",
            "format": "integer"
          },
          {
            "label": "Digital (CCMS)",
            "groupId": "digital",
            "format": "integer"
          }
